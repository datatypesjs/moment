{
  "name": "@datatypes/moment",
  "version": "0.8.1",
  "description": "ISO 8601 based time and date module",
  "main": "build/index",
  "directories": {
    "lib": "source",
    "test": "test"
  },
  "scripts": {
    "lint": "eslint --ignore-path .gitignore .",
    "build": "babel --quiet source --out-dir build",
    "unit-tests": "npm run build && ava",
    "test": "npm run lint && npm run unit-tests",
    "prepublish": "npm test && npm run build"
  },
  "keywords": [
    "iso 8601",
    "time",
    "date"
  ],
  "author": "Adrian Sieber",
  "license": "MIT",
  "dependencies": {
    "@datatypes/duration": "^0.4.0",
    "end-of-week": "^1.0.0",
    "month-days": "^1.0.0",
    "zpad": "^0.5.0"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-preset-latest": "^6.16.0",
    "babel-register": "^6.18.0",
    "eslint": "^3.11.1",
    "javascript-styleguide": "^1.0.0",
    "unexpected": "^10.20.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/datatypesjs/moment.git"
  },
  "bugs": {
    "url": "https://github.com/datatypesjs/moment/issues"
  },
  "homepage": "https://github.com/datatypesjs/moment#readme",
  "eslintConfig": {
    "root": true,
    "extends": "./node_modules/javascript-styleguide/.eslintrc.yaml",
    "rules": {
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1
        }
      ]
    }
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}

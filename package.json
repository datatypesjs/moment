{
  "name": "@datatypes/moment",
  "version": "0.2.0",
  "description": "ISO 8601 based time and date module",
  "main": "build/index",
  "directories": {
    "lib": "source",
    "test": "test"
  },
  "scripts": {
    "build": "babel --quiet source --out-dir build",
    "test": "ava --require babel-register --fail-fast",
    "prepublish": "npm test && npm run build"
  },
  "keywords": [
    "iso 8601",
    "time",
    "date"
  ],
  "author": "Adrian Sieber",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.12.0",
    "babel-cli": "^6.6.5",
    "babel-plugin-syntax-trailing-function-commas": "^6.5.0",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.6.5",
    "unexpected": "^10.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/datatypesjs/moment.git"
  },
  "bugs": {
    "url": "https://github.com/datatypesjs/moment/issues"
  },
  "homepage": "https://github.com/datatypesjs/moment#readme",
  "dependencies": {
    "@datatypes/duration": "^0.2.0",
    "zpad": "^0.5.0"
  }
}
